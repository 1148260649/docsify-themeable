{
  "name": "docsify-themeable",
  "version": "0.0.1",
  "description": "A delightfully simple theme system for docsify.js",
  "author": "John Hildenbiddle <http://hildenbiddle.com>",
  "license": "MIT",
  "homepage": "https://github.com/jhildenbiddle/docsify-themeable",
  "repository": {
    "type": "git",
    "url": "git+https://jhildenbiddle@github.com/jhildenbiddle/docsify-themeable.git"
  },
  "bugs": {
    "url": "https://github.com/jhildenbiddle/docsify-themeable/issues"
  },
  "keywords": [
    "css",
    "docsify",
    "sass",
    "scsss",
    "theme",
    "themeable"
  ],
  "browserslist": [
    "ie >= 10"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/js/docsify-themeable.js",
  "unpkg": "dist/js/docsify-themeable.min.js",
  "scripts": {
    "prepare": "run-s clean build",
    "build": "run-p build:*",
    "build:js": "rollup -c",
    "build:pcss": "postcss build/css/**/*.css --dir dist/css --use postcss-remove-null postcss-discard-empty postcss-flexbugs-fixes autoprefixer --map",
    "build:scss": "node-sass --output-style compressed src/scss/themes -o build/css --source-map true --source-map-contents true",
    "clean": "rimraf build/* dist/* docs/css docs/js",
    "serve": "node server.js",
    "start": "run-p watch serve",
    "watch": "run-p 'build:* -- -w'",
    "prewatch": "npm run build:scss",
    "version": "npm run build && git add -A dist"
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.18.13",
    "compression": "^1.7.2",
    "css-vars-ponyfill": "^1.3.0",
    "eslint": "^4.19.1",
    "eslint-plugin-html": "^4.0.3",
    "lodash.merge": "^4.6.1",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "postcss": "^6.0.21",
    "postcss-cli": "^5.0.0",
    "postcss-discard-empty": "^2.1.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-remove-null": "^1.2.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
